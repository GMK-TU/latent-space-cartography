{
  "dataset": "emoji",
  "data_type": "image",
  "train_split": 13500,
  "initial_dim": 32,
  "initial_projection": "t-SNE",
  "dims": [4, 8, 16, 32, 64, 128],
  "schema": {
    "type": {},
    "meta": ["i","name", "mean_color", "category", "platform", "version", "codepoints", "shortcode"]
  },
  "rendering": {
    "dot_color": "mean_color",
    "ext": "png"
  },
  "search": {
    "advanced": true,
    "by": ["name", "codepoints", "shortcode"],
    "filter": "platform"
  }
}